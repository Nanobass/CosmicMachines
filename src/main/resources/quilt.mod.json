{
  "schema_version": 1,

  "quilt_loader": {
    "group": "${mod_group}",
    "id": "${mod_id}",
    "version": "${mod_version}",

    "intermediate_mappings":"net.fabricmc:intermediary",

    "metadata": {
      "name": "${mod_name}",
      "description": "${mod_desc}",
      "contributors": {
        "Nanobass": "Author"
      },
      "license": "GPL-3.0-or-later",
      "contact": {
        "issues": "https://github.com/Nanobass/CosmicMachines/issues",
        "sources": "https://github.com/Nanobass/CosmicMachines",
        "github": "https://github.com/Nanobass"
      }
    },

    "entrypoints": {
      "main": "net.paxyinc.MachineMod",
      "craterchatplugin": "net.paxyinc.MachinesCraterChatPlugin"
    },

    "depends": [
      {
        "id": "cosmicquilt",
        "versions": "*"
      },
      {
        "id": "cosmicreach",
        "versions": ">=${cosmic_reach_version}"
      }
    ]
  },

  "mixin": "cosmicmachines.mixins.json"
}
